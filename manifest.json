{
  "manifest_version": 3,
  "name": "Atenea Dark Mode (UPC)",
  "version": "1.3",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["*://atenea.upc.edu/*"],
      "css": ["styles.css"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["logo.png", "logo-upc.png"],
      "matches": ["*://atenea.upc.edu/*"]
    }
  ]
}